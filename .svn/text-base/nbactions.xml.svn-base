                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath org.mousephenotype.impress.owl.App C:/wamp/www/impress_owl/impress-owl.owl jdbc:mysql://root:Ops_Ram@localhost:3306/impress</exec.args>
